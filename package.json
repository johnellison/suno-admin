  "version": "0.1.0",
  "description": "AI-powered music prompt generator for Suno v5 - Creates focus music albums with harmonic mixing",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "suno-admin": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/cli.ts",
    "start": "node dist/cli.js",
    "generate": "tsx src/cli.ts generate",
    "analyze": "tsx src/cli.ts analyze",
    "convert": "tsx src/cli.ts convert",
    "frequencies": "tsx src/cli.ts frequencies"
